{
  "name": "mini-chef-app",
  "version": "0.1.0",
  "private": true,
  "description": "React web-app to manage cooking recipes",
  "homepage": "https://cosminmihai87.github.io/mini-chef-app",
  "main": "./build/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all lint-fix webpack-development",
    "build": "cross-env NODE_ENV=production npm-run-all lint-check webpack-production",
    "webpack-development": "webpack-dev-server",
    "webpack-production": "webpack --config webpack.config.js",
    "lint-check": "npm-run-all eslint-check stylelint-check",
    "lint-fix": "npm-run-all eslint-fix stylelint-fix",
    "eslint-check": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint-fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "stylelint-check": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint-fix": "stylelint --fix \"src/**/*.{css,scss}\"",
    "test": "jest --verbose --updateSnapshot --coverage=false",
    "test-coverage": "jest --coverage",
    "test-single-file": "npx jest -u -- \"App.test.tsx\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "formik": "^2.2.9",
    "gh-pages": "^3.2.3",
    "react": "^18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.1.0",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^6.3.0",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.2",
    "resolve": "^1.20.0",
    "styled-components": "^5.3.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-redux": "^7.1.24",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "react-app-polyfill": "^3.0.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.3.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.1",
    "stylelint-config-standard-scss": "^3.0.0",
